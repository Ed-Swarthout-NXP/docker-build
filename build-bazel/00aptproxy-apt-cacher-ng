# use a apt-cacher-ng proxy for packages
# to use, add correct IP address to /etc/hosts for apt-cacher-ng
Acquire::http::Proxy "http://apt-cacher-ng:3142";

#exclusions:
Acquire::HTTP::Proxy::download.docker.com "DIRECT";
Acquire::HTTP::Proxy::download.oracle.com "DIRECT";
Acquire::HTTP::Proxy::packages.cloud.google.com "DIRECT";
Acquire::HTTP::Proxy::packages.microsoft.com "DIRECT";
